    "url": "https://rhynl.io"
  },
  "main": "dist/lib/pino-loggly.js",
  "types": "dist/lib/pino-loggly.d.ts",
  "bin": "dist/cli.js",
  "engines": {
    "node": ">=10",
    "npm": ">=6.14.0",
    "vscode": ">=1.45.0"
  },
  "scripts": {
